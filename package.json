{
  "name": "qactus-e2e",
  "version": "1.0.0",
  "description": "A React web application with end-to-end testing using Playwright and TypeScript",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:chrome": "playwright test --project=chromium",
    "e2e:firefox": "playwright test --project=firefox",
    "e2e:safari": "playwright test --project=webkit",
    "e2e:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:e2e": "eslint tests/e2e --ext .ts",
    "lint:e2e:fix": "eslint tests/e2e --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "keywords": [
    "react",
    "typescript",
    "playwright",
    "e2e",
    "testing"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-playwright": "^0.19.0",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "react-scripts": "^5.0.1"
  }
}
